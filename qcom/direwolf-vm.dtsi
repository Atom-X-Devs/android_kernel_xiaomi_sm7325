	};

	gcc_pcie_4_gdsc: gcc_pcie_4_gdsc {
		regulator-name = "gcc_pcie_4_gdsc";
	};

	L3A0: pm8540_a0_l3: regulator-pm8540_a0-l3 {
		regulator-name = "ldoa3";
