				};
			};
		};

		lpi_i2s1_ws {
			lpi_i2s1_ws_sleep: lpi_i2s1_ws_sleep {
				mux {
					pins = "gpio7";
					function = "func2";
				};

				config {
					pins = "gpio7";
					drive-strength = <2>;   /* 2 mA */
					bias-pull-down;         /* PULL DOWN */
					input-enable;
				};
			};

			lpi_i2s1_ws_active: lpi_i2s1_ws_active {
