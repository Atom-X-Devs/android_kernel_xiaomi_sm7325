&reserved_memory {
	removed_mem: removed_region@c0000000 {
		reg = <0x0 0xc0000000 0x0 0x6800000>;
	};
};

/*
 * PM7325B device specific diff for Xiaomi Redwood platform
 */
&pm7325b_gpios {
	// Disable PM7325B GPIO_2_3 to fix inaccurate temp value

	pm7325b_chg_skin_therm {
		pm7325b_chg_skin_therm_default: pm7325b_chg_skin_therm_default {
			pins = "gpio2";
			bias-high-impedance;
		};
	};

	pm7325b_pa_rfendc_therm {
		pm7325b_pa_rfendc_therm_default: pm7325b_pa_rfendc_therm_default {
			pins = "gpio3";
			bias-high-impedance;
		};
	};
};

/*
 * PM8350C devices specific diff for Xiaomi Redwood platform
 */
&L6C {
	regulator-boot-on; /* M20 for RF ANT Detect MXD8625C Switch IC */
	regulator-always-on;
	regulator-min-microvolt = <2900000>;
	regulator-max-microvolt = <2900000>;
	qcom,init-voltage = <2900000>;
};

/*
 * PMK8350 device specific diff for Xiaomi Redwood platform
 */
&pmk8350 {
	pm8350_adc_tm_iio: adc_tm_iio@3400 {
		compatible = "qcom,adc-tm7-iio";
		reg = <0x3400>;
		#thermal-sensor-cells = <1>;
		#address-cells = <1>;
		#size-cells = <0>;

		io-channels = <&pmk8350_vadc PM7325B_ADC7_AMUX_THM4_100K_PU>,
					  <&pmk8350_vadc PM7325B_ADC7_GPIO3_100K_PU>;

		pm7325b_conn_therm {
			reg = <PM7325B_ADC7_AMUX_THM4_100K_PU>;
			qcom,ratiometric;
			qcom,hw-settle-time = <200>;
		};

		pm7325b_pa_rfendc_therm {
			reg = <PM7325B_ADC7_GPIO3_100K_PU>;
			qcom,ratiometric;
			qcom,hw-settle-time = <200>;
		};
	};
};

&pmk8350_vadc {
	// Disable PM7325B GPIO_2_3 to fix inaccurate temp value
	pinctrl-names = "default";
	pinctrl-0 = <&pm7325b_chg_skin_therm_default>;
	pinctrl-1 = <&pm7325b_pa_rfendc_therm_default>;

	pm7325b_conn_therm {
		reg = <PM7325B_ADC7_AMUX_THM4_100K_PU>;
		label = "pm7325b_conn_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pm7325b_pa_rfendc_therm {
		reg = <PM7325B_ADC7_GPIO3_100K_PU>;
		label = "pm7325b_pa_rfendc_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&thermal_zones {
	conn_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8350_adc_tm_iio PM7325B_ADC7_AMUX_THM4_100K_PU>;
		wake-capable-sensor;

		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	pa_4gtherm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pmk8350_adc_tm PM7325_ADC7_AMUX_THM5_100K_PU>;
		wake-capable-sensor;

		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	pa_5gtherm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pmk8350_adc_tm PM7325_ADC7_GPIO3_100K_PU>;
		wake-capable-sensor;

		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	pa_rfendc_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8350_adc_tm_iio PM7325B_ADC7_GPIO3_100K_PU>;
		wake-capable-sensor;

		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};

// Awinic AW88261 Smart Amp
&qupv3_se3_i2c {
	status = "ok";

	/* TOP Smart PA */
	aw882xx_smartpa@35 {
		compatible = "awinic,aw882xx_smartpa";
		reg = <0x35>;
		irq-gpio = <&tlmm 102 0>;
		interrupt-parent = <&tlmm>;
		interrupts = <102 0>;
		interrupt-names = "smartpa_irq";
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&top_smartpa_int_active>;
		pinctrl-1 = <&top_smartpa_int_suspend>;
		aw-re-min = <4000>;
		aw-re-max = <30000>;
		aw-cali-mode = "aw_class";
		sound-channel = <0>;
		status = "ok";
	};

	/* BOT Smart PA */
	aw882xx_smartpa@34 {
		compatible = "awinic,aw882xx_smartpa";
		reg = <0x34>;
		irq-gpio = <&tlmm 45 0>;
		interrupt-parent = <&tlmm>;
		interrupts = <45 0>;
		interrupt-names = "smartpa_irq";
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&bot_smartpa_int_active>;
		pinctrl-1 = <&bot_smartpa_int_suspend>;
		aw-re-min = <4000>;
		aw-re-max = <30000>;
		aw-cali-mode = "aw_class";
		sound-channel = <1>;
		status = "ok";
	};
};

&qupv3_se4_i2c {
	status = "ok";
	qcom,clk-freq-out = <1000000>;
	#address-cells = <1>;
	#size-cells = <0>;
	nq@28 {
		compatible = "qcom,sn-nci";
		reg = <0x28>;
		qcom,sn-irq = <&tlmm 41 0x00>;
		qcom,sn-ven = <&tlmm 38 0x00>;
		qcom,sn-firm = <&tlmm 40 0x00>;
		qcom,sn-clkreq = <&tlmm 39 0x00>;
		qcom,sn-vdd-1p8-supply = <&L18B>;
		qcom,sn-vdd-1p8-voltage = <1800000 1800000>;
		qcom,sn-vdd-1p8-current = <157000>;
		interrupt-parent = <&tlmm>;
		interrupts = <41 0>;
		interrupt-names = "nfc_irq";
		pinctrl-names = "nfc_active", "nfc_suspend";
		pinctrl-0 = <&nfc_int_active &nfc_enable_active>;
		pinctrl-1 = <&nfc_int_suspend &nfc_enable_suspend>;
	};
};

