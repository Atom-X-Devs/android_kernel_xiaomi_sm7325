		cam_vdig-supply = <&L1I>;
		cam_vana-supply = <&L6I>;
		cam_vaf-supply = <&L3I>;
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;
		cam_bob-supply = <&BOB>;

