		};
	};

	qcom,pm8540@9 {
		compatible ="qcom,spmi-pmic";
		reg = <9 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;
	};

	/* below definitions are for the forth instance of pm8540 */
	qcom,pm8540@C {
		compatible = "qcom,spmi-pmic";
		reg = <12 SPMI_USID>;
