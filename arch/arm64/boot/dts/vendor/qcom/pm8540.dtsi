		pm8540_2_clkdiv: clock-controller@5b00 {
			compatible = "qcom,spmi-clkdiv";
			reg = <0x5b00>;
			#clock-cells = <1>;
			qcom,num-clkdivs = <2>;
			clock-output-names = "pm8540_2_div_clk1",
						"pm8540_2_div_clk2";
			clocks = <&rpmhcc RPMH_CXO_CLK>;
